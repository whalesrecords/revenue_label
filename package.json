{
  "name": "csv-merge",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm install",
    "build": "npm run install:client && npm run install:server && npm run build:client && npm run build:server"
  },
  "dependencies": {
    "@netlify/functions": "^3.1.8"
  }
}
